{
  "name": "unfuckthesystem.dk",
  "version": "0.0.1",
  "author": "noahsturis",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "lint": "eslint . --ext .ts,.vue",
    "prepare": "husky install",
    "format-check": "npx prettier --check ./",
    "format-write": "npx prettier --write ./",
    "test": "vitest",
    "test:e2e": "testcafe chrome tests/e2e/",
    "vercel-build": "prisma generate && npx prisma migrate deploy && nuxt build",
    "prisma:generate": "npx prisma generate",
    "drizzle:generate": "drizzle-kit generate"
  },
  "bin": {
    "cli": "./utils/cli.ts"
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "bun run lint"
    ]
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.0.2",
    "@nuxt/eslint": "^0.5.5",
    "@nuxt/test-utils": "^3.0.0-rc.3",
    "@nuxtjs/eslint-config-typescript": "^10.0.0",
    "@pinia/nuxt": "^0.4.0",
    "@types/pg": "^8.11.7",
    "@typescript-eslint/typescript-estree": "^8.3.0",
    "@vueuse/nuxt": "^8.3.1",
    "axios": "^0.27.2",
    "cz-conventional-changelog": "3.3.0",
    "drizzle-kit": "^0.24.2",
    "drizzle-prisma-generator": "^0.1.7",
    "eslint": "^9.9.1",
    "husky": "^7.0.0",
    "lint-staged": "^12.4.1",
    "msw": "^0.39.2",
    "nuxt": "^3.11.0",
    "pinia": "^2.0.13",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.10",
    "tailwindcss": "^3.1.7",
    "testcafe": "^1.18.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "vitest": "^0.12.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "msw": {
    "workerDirectory": "public"
  },
  "dependencies": {
    "@headlessui/vue": "^1.6.7",
    "@heroicons/vue": "^1.0.6",
    "@nuxt/kit": "^3.13.0",
    "@nuxt/ui": "^2.11.0",
    "@typoas/runtime": "^3.1.1",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.33.0",
    "node-fetch": "^3.3.2",
    "node-html-parser": "^6.1.11",
    "pg": "^8.12.0",
    "winston": "^3.11.0"
  }
}
